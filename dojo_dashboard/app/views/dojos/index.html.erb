<h1>Listing <%= @count%> Dojos</h1>
<h2><%= flash[:success]%></h2>
<table>
    <tr>
        <th>Coding Dojo Branch</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
        <th>Actions</th>
    </tr>
    <% @dojos.each do |dojo| %>
    <tr>
        <td><%= dojo.branch%></td>
        <td><%= dojo.street%></td>
        <td><%= dojo.city%></td>
        <td><%= dojo.state%></td>
        <td><a href="/dojos/<%= dojo.id%>">Show</a> | <a href="/dojos/id/edit">Edit</a> | <a href="dojos/id">Destroy</a> </td>
    </tr>
    <% end %>
</table><br><br>
<a href="/dojos/new">new dojo</a>